@model Lab3.Models.Person
@{
    Layout = "_Layout";
    ViewData["Title"] = "Display Person Details";
}
<h3>Display Person Details</h3>
<table>
    <tr>
        <td>
            Last Name:
        </td>
        <td>
            @Model.LastName
        </td>
    </tr>
    <tr>
        <td>
            First Name:
        </td>
        <td>
            @Model.FirstName
        </td>
    </tr>
    <tr>
        <td>
            Age:
        </td>
        <td>
            @Model.Age
        </td>
    </tr>
    <tr>
        <td>
            Email:
        </td>
        <td>
            @Model.EmailAddress
        </td>
    </tr>
    <tr>
        <td>
            Description:
        </td>
        <td>
            @Model.Description
        </td>
    </tr>

</table>